{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "max-classes-per-file": [false],
    "file-name-casing": [
      true,
      {
        ".ts": "camel-case",
        ".d.ts": "kebab-case",
        ".*": "camel-case"
      }
    ]
  }
}
